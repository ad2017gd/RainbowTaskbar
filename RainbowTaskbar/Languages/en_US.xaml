                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <!-- General -->
    <s:String x:Key="name">RainbowTaskbar</s:String>
    <s:String x:Key="editor">Editor</s:String>
    <s:String x:Key="settings">Settings</s:String>
    <s:String x:Key="about">About</s:String>

    <!-- Editor Tab -->
    <s:String x:Key="add">Add</s:String>
    <s:String x:Key="remove">Remove</s:String>

    <s:String x:Key="apply">Apply</s:String>
    <s:String x:Key="applyandsave">Apply and Save</s:String>
    <s:String x:Key="topreset">To preset</s:String>

    <!-- Settings Tab -->
    <s:String x:Key="save">Save</s:String>
    <s:String x:Key="general">General</s:String>
    <s:String x:Key="sstartup">Run at system startup</s:String>
    <s:String x:Key="supdates">Check for updates</s:String>
