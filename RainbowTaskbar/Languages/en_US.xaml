    <s:String x:Key="transparencyinstructionstyleformat">Style - {0}</s:String>
    <s:String x:Key="transparencyinstructionlayerformat">Layer {0} - {1}%</s:String>
    <s:String x:Key="transparencyinstructionopacityformat">{0} - {1}%</s:String>

    <!-- Enums -->
        <!-- Shapes -->
    <s:String x:Key="enum_line">Line</s:String>
    <s:String x:Key="enum_rectangle">Rectangle</s:String>
    <s:String x:Key="enum_roundedrect">Rounded rectangle</s:String>
    <s:String x:Key="enum_ellipse">Ellipse</s:String>
        <!-- Color instruction effects -->
    <s:String x:Key="enum_solid">Solid</s:String>
    <s:String x:Key="enum_fade">Fading solid</s:String>
    <s:String x:Key="enum_gradient">Gradient</s:String>
    <s:String x:Key="enum_fadegradient">Fading gradient</s:String>
        <!-- Color instruction transitions -->
    <s:String x:Key="enum_linear">Linear</s:String>
    <s:String x:Key="enum_cubic">Cubic</s:String>
