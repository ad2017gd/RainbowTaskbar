    <s:String x:Key="borderradiusinstructionformat">Border Radius - {0}px</s:String>
    <s:String x:Key="clearlayerinstructionformat">Clear layer - {0}</s:String>
    <s:String x:Key="colorinstructionrandomizedformat">{0} - Randomized</s:String>
    <!-- no translation --> <s:String x:Key="colorinstructiongradientformat">{0} - {1} -> {2}</s:String>
