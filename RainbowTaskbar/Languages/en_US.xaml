    <s:String x:Key="randomize">Randomize colors</s:String>
    <s:String x:Key="holdtime">Hold time</s:String>
    <s:String x:Key="fadetime">Fade time</s:String>
    <s:String x:Key="gradientangle">Gradient angle °</s:String>
    <s:String x:Key="layer">Layer</s:String>
    <s:String x:Key="milliseconds">ms</s:String>
        <!-- Image -->
    <s:String x:Key="chooseimg">Choose image</s:String>
    <s:String x:Key="width">Width</s:String>
    <s:String x:Key="height">Height</s:String>
        <!-- Text -->
    <s:String x:Key="fontsize">Font size</s:String>
    <s:String x:Key="fontcolor">Font color</s:String>
    <s:String x:Key="centerx">Center on X-axis</s:String> <!-- center is a verb! -->
        <!-- Transparency -->
    <s:String x:Key="change">Change</s:String>
    <s:String x:Key="to">to</s:String>



    <!-- Instruction context menu -->
    <s:String x:Key="duplicate">Duplicate</s:String> <!-- it's a verb! -->
